<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <plurals name="TAGS_LIST_deleting_dialog_title">
        <item quantity="one">Delete %1$d selected tag?</item>
        <item quantity="other">Delete %1$d selected tags?</item>
    </plurals>

    <plurals name="TAGS_LIST_cannot_delete_more_tags_at_once">
        <item quantity="one">Cannot delete more then %1$d tag at once</item>
